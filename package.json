{
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "optimize": "vite optimize --force",
    "lint": "xo",
    "lint-fix": "xo --fix"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.41.0",
    "eslint-plugin-vue": "^9.14.0",
    "unplugin": "^1.3.1",
    "unplugin-element-plus": "^0.7.1",
    "vite": "^4.3",
    "vite-plugin-symfony": "^0.7.8",
    "vue-eslint-parser": "^9.3.0",
    "xo": "^0.54.2"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.4.0",
    "axios-cache-interceptor": "^1.1.1",
    "element-plus": "^2.3.5",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "pinia": "^2.1.3",
    "sass": "^1.62.1",
    "validator": "^13.9.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1"
  },
  "xo": {
    "space": true,
    "env": [
      "shared-node-browser",
      "browser",
      "es2022"
    ],
    "rules": {
      "camelcase": 0,
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "kebabCase": true,
            "pascalCase": true
          }
        }
      ],
      "unicorn/prefer-top-level-await": "off"
    },
    "plugins": [
      "vue"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2022,
      "ecmaFeatures": {
        "globalReturn": false,
        "impliedStrict": false,
        "jsx": false
      }
    },
    "extensions": [
      "vue",
      "js",
      "jsx",
      "cjs",
      "mjs",
      "ts"
    ]
  }
}
